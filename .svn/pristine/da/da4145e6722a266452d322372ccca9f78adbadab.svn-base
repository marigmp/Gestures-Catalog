 .jqTransformSelectWrapper ul a:hover, .jqTransformSelectWrapper ul a.selected:hover { background: #f1f2f4; color: #333333; }
 .jqTransformHidden {display: none;}
 .jqTransformButton {display: none;}
 .jqTransformTextarea { display:block; height:105px; }
 .wsite-search-element input { -webkit-border-radius: 15px/15px 15px 14px 14px; -moz-border-radius: 15px/15px 15px 14px 14px; border-radius: 15px/15px 15px 14px 14px; background-color: #fff; border: solid 1px #ccc; font-size:14px; padding:7px 30px 7px 11px !important; }
 .wsite-search-element-submit { background:url(theme/icon-search.png?1410197657) no-repeat; width:18px; height:16px; margin-top:-9px; }
 .wsite-button, .wsite-button-highlight { display: inline-block; padding: 0px; background: #669933 !important; }
 .wsite-button-inner { color: #fff; font-family: 'Dosis', sans-serif; display: block; font-size: 27px; font-weight: 500; letter-spacing:-1px; word-spacing:4px; text-transform: uppercase; padding: 0px 30px; background: #669933 !important; height:67px; line-height:67px; }
 .wsite-button:hover .wsite-button-inner { background:#a2ba1e !important; }
 .wsite-button:active .wsite-button-inner { background:#b8cb52 !important; color:#000; }
 .wsite-button-large, .wsite-button-large.wsite-button-highlight { padding: 0px; }
 .wsite-button-large .wsite-button-inner { font-size:35px; padding: 0px 45px; background: #669933 !important; height: 88px; line-height:88px; }
 .wsite-button-large:hover .wsite-button-inner { background:#a2ba1e !important; }
 .wsite-button-large:active .wsite-button-inner { background:#b8cb52 !important; }
 .wsite-button-highlight .wsite-button-inner { background: #000 !important; }
 .wsite-button-highlight:hover .wsite-button-inner { background:#693 !important; }
 .wsite-button-large.wsite-button-highlight .wsite-button-inner { background: #000 !important; }
 .wsite-button-large.wsite-button-highlight:hover .wsite-button-inner { background:#693 !important; }

div.paragraph ul, div.paragraph ol { padding-left: 2.3em !important; margin: 5px 0 !important; overflow:hidden; }
div.paragraph li { padding-left: 5px !important; margin: 3px 0 0 !important; }
